	<script>
	$('input:radio[name=namecheck]').on('change', function() {
		var idx = $('input:radio[name=namecheck]').index(this);
		$("#sAuthType").val(idx+1);
		$(".auth").val('');
		if(idx==0){
			MemberCommonUtil.inputInitObj($("#tnName"));
			MemberCommonUtil.inputInitObj($("#tn"), 'search');
			MemberCommonUtil.inputInitObj($("#tnSignKey"));
			$("#isSmsSend").val('');
		}else if(idx==1){
			MemberCommonUtil.inputInitObj($("#emailName"));
			MemberCommonUtil.inputInitEmail($('#email'), $('#mailDomain'), 0, 'search');
			MemberCommonUtil.inputInitObj($("#emailSignKey"));
			$("#isEmailSend").val('');
		}
	});

	$('input').on('change',function() {
		if($(this).val() != ''){
			var id = $(this).attr("id");
			MemberValid.params = {	isSystemAlert:4	};
			MemberServerFunc.params = {	isSystemAlert:4	};
			if('tnName' == id){
				MemberValid.simpleNameCheck("#tnName");
			}else if('tn' == id){
				MemberValid.phoneNumberCheck("#tn","1");
				MemberCommonUtil.inputInitObj($("#tnSignKey"), 'search');
				$("#isSmsSend").val('');
				$(".auth").val('');
			}else if('emailName' == id){
				MemberValid.simpleNameCheck("#emailName");
			}else if('email' == id){
				MemberValid.emailCheck("#email",undefined,undefined,false,false,true);
				MemberCommonUtil.inputInitObj($("#emailSignKey"), 'search');
				$("#isEmailSend").val('');
				$(".auth").val('');
			}else if('emailSignKey' == id){
				if(MemberValid.authorizationCodeCheck("#emailSignKey")){
					MemberServerFunc.certifyAuthorizationCode("1", "#emailSignKey", "#emailAuthKey", "#sendTime");
				}
			}else if('tnSignKey' == id){
				if(MemberValid.authorizationCodeCheck("#tnSignKey")){
					MemberServerFunc.certifyAuthorizationCode("2", "#tnSignKey", "#smsAuthKey", "#sendTime");
				}
			}
		}
    });

    $('body').on('click','.btn_send_num',function() {
    	var isNameCheck = isTnCheck = isEmailCheck = false;
    	MemberValid.params = {isSystemAlert : "4"};

      	if($("#sAuthType").val() == "2"){
      		var isNameCheck = MemberValid.simpleNameCheck("#emailName");
			var isEmailCheck = MemberValid.emailCheck("#email",undefined,undefined,false,false,true);
			isTnCheck = true;
      	}else{
      		isNameCheck = MemberValid.simpleNameCheck("#tnName");
			isTnCheck = MemberValid.phoneNumberCheck("#tn","1");
			isEmailCheck = true;
      	}

    	if(isNameCheck && isTnCheck && isEmailCheck){
    		memAuthKeySend();
    	}else{
    		$(this).parents().find(".singKey_check").hide();
    	}
    });
    
    $('.d_input_del').on('click',function(e){
    	var id = $(this).parents('.d_input_write').find('input').attr("id");
    	if("email"==id) {
    		MemberCommonUtil.inputDelEmail($('#email'), $('#mailDomain'), 0, 'search');
    		$("#isEmailSend").val('');
			$(".auth").val('');
    	}else if("tn"==id) {
    		MemberCommonUtil.inputDelObj(this, 'search');
    		$("#isSmsSend").val('');
			$(".auth").val('');
    	}else {
    		MemberCommonUtil.inputDelObj(this);
    	}
   	});

    function memAuthKeySend(){
    	var authType = $("#sAuthType").val();
    	var type ="1";
    	var msgEleId = "#tnSignKey";
    	var checkSendType = "SMS";
    	var checkSendVal = $("#tn").val();
    	var memberName = $("#tnName").val();

    	if(authType == "2"){
    		type ="2";
        	msgEleId = "#emailSignKey";
        	checkSendType = "EMAIL";
        	checkSendVal = $("#email").val();
        	memberName = $("#emailName").val();
    	}
    	
   		$(".auth").val('');
    	MemberCommonUtil.inputInitObj(authType == "2"? $("#emailSignKey") : $("#tnSignKey"));
		var isResend = authType=="2"?$("#isEmailSend").val()!="":$("#isSmsSend").val()!="";
		MemberCommonUtil.initTimer();
		MemberServerFunc.params = {
       		isSystemAlert: 4,
       		msgCssElementId: msgEleId+"_tr",
       		msgElementId: msgEleId
       	};
		MemberServerFunc.sendAuthorizationCode(checkSendVal,checkSendType,"","","M001",memberName);
		var infoMsg = "";
		if($("#sendError").val()==""){
			if(isResend){
				if(authType == "2") $("#EMAIL_SEND_SIGNKEY").html("인증번호 재전송");
				else $("#SNS_SEND_SIGNKEY").html("인증번호 재전송");
				
				infoMsg = (authType=="2"?"해당 이메일":"해당 번호")+"로 인증번호를 재발송했습니다.";
				alert(infoMsg);
			}else{
				infoMsg = (authType=="2"?"해당 이메일":"해당 번호")+"로 인증번호를 발송했습니다.";
			}
			$(msgEleId+"_info").text(infoMsg);
			$(msgEleId).parents().parents('.singKey_check').show();
		}else{
			$(msgEleId+"_info").text("");
			$(msgEleId).parents().parents('.singKey_check').hide();			
		}
    }

	function selfAuthLog(type){
		var paramStr = "";
		if($("#tr_log").length>0){
			paramStr = "tr_log="+encodeURIComponent( $("#tr_log").val() )+"&realnameType="+type+"&cpId=WP10";
		}else{
			paramStr = "realnameType="+type;
		}
		$.ajax({
			url : "/muid/web/search/idsearch_ajaxRealnameCountLogger.json",
			data : paramStr,
			type : "POST",
			async : true,
			success : function(date){},
			error: function(e){}
		});
	}

	function idSearch(){
		MemberValid.params = {	isSystemAlert:4	};
		MemberServerFunc.params = {	isSystemAlert:4	};
		var arrayValidFunction = new Array();
		var arrayValidElement = new Array();
		var stepUrl = "";
		var urlTarget = "";
		var windowName = "";
		var popWindowName = "";
		var popWidth = 0;
		var popHeight = 0;

		var authType = $("#sAuthType").val();
		if(authType==""){
    		alert("아이디 찾기 방법을 선택해주세요.");
    		return;
    	}

		if(authType == "1"){ //tn
			var isCheck = false;
			var isNameCheck = MemberValid.simpleNameCheck("#tnName");
			var isTnCheck = MemberValid.phoneNumberCheck("#tn");
			if(isNameCheck && isTnCheck){
				if(MemberValid.isSendCheck("#isSmsSend")){
					if(MemberValid.authorizationCodeCheck("#tnSignKey")){
						if(MemberServerFunc.certifyAuthorizationCode("2", "#tnSignKey", "#smsAuthKey", "#sendTime")){
							isCheck=true;
							arrayValidFunction.push("simpleNameCheck");
							arrayValidFunction.push("phoneNumberCheck");
							arrayValidFunction.push("isSendCheck");
							arrayValidFunction.push("authorizationCodeCheck");
							arrayValidFunction.push("certifyAuthorizationCode");
							arrayValidElement.push("#tnName");
							arrayValidElement.push(new Array("#tn","1"));
							arrayValidElement.push("#isSmsSend");
				    		arrayValidElement.push("#tnSignKey");
				    		arrayValidElement.push(new Array("2", "#tnSignKey", "#smsAuthKey", "#sendTime"));
						}
					}
				}
			}
			if(isCheck == false){
				return;
			}
			stepUrl = httpsMemberDomain+MemberEtc.changeUrl("/muid/web/search/idsearch_listView.htm");
		}else if(authType == "2"){	//email
			var isCheck = false;
			var isNameCheck = MemberValid.simpleNameCheck("#emailName");
			var isEmailnCheck = MemberValid.emailCheck("#email",undefined,undefined,false,false,true);
			if(isNameCheck && isEmailnCheck){
				if(MemberValid.isSendCheck("#isEmailSend")){
					if(MemberValid.authorizationCodeCheck("#emailSignKey")){
						if(MemberServerFunc.certifyAuthorizationCode("1", "#emailSignKey", "#emailAuthKey", "#sendTime")){
							isCheck=true;
							arrayValidFunction.push("simpleNameCheck");
							arrayValidFunction.push("emailCheck");
							arrayValidFunction.push("isSendCheck");
							arrayValidFunction.push("authorizationCodeCheck");
							arrayValidFunction.push("certifyAuthorizationCode");
							arrayValidElement.push("#emailName");
							arrayValidElement.push(new Array("#email",undefined,undefined,false,false,false));
							arrayValidElement.push("#isEmailSend");
							arrayValidElement.push("#emailSignKey");
							arrayValidElement.push(new Array("1", "#emailSignKey", "#emailAuthKey", "#sendTime"));
						}
					}
				}
			}
			if(isCheck == false){
				return;
			}
 			stepUrl = httpsMemberDomain+MemberEtc.changeUrl("/muid/web/search/idsearch_listView.htm");
		}else if(authType == "3"){	//ipin
			$("#methodType").val("IPIN");
			stepUrl = $("#ipinUrl").val();
			urlTarget = "OPENER";
			windowName = "_IPIN_IDPWD_SEARCH";
			popWindowName = "_POP_IDPWD_SEARCH";
			popWidth = 420;
			popHeight = 555;
			selfAuthLog($("#methodType").val());
		}else if(authType == "4"){	//phone auth
			$("#methodType").val("KMC");
			stepUrl = $("#kmcUrl").val();
			urlTarget = "OPENER";
			windowName = "_KMC_IDPWD_SEARCH";
			popWindowName = "_POP_IDPWD_SEARCH";
			popWidth = 440;
			popHeight = 575;
			selfAuthLog($("#methodType").val());
		}else{
			alert("인증방법을 선택 해 주세요.");
			return false;
		}

		MemberIdPwdSearch.params = {
			formName : "search_form",		// 폼 객체
			stepUrl : stepUrl,				// 다음단계 URL
			urlTaget : urlTarget,			// URL TARGET(OPENER NAME,IFRAME NAME,SELF);
			windowName : windowName,		// 바닥창이름
			popWindowName : popWindowName,	// 팝업창이름
			popWidth : popWidth,			// 팝업창 넓이
			popHeight : popHeight,			// 팝업창 높이
			isFormValid : false,			// form valid true or false
			validFunction : arrayValidFunction,		//valid function
			validElement : arrayValidElement,		//valid elementId or elementName
			validCheckType : 1,				// valid check type(1:step by step check,2:only one check)
			isSystemAlert : 4,				// system alert(default:system alert message,2 : innerHTML message)
			afterFunction : undefined		//valid check 이후 submit이전에 실행되어야 할 함수
		};

		MemberIdPwdSearch.action();
	}
	</script>
